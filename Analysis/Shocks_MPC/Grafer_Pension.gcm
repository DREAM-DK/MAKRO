  a(p:vHHInd)[2085]-a(@vHHInd)[2085] 'Indkomst (vHhInd) - tværsnit i år 60 (2085)',
  (p:vHHInd[tot][2026]-@vHHInd[tot][2026])/@nPop[tot][2026] 'Gns. indkomst (vHhInd) - tværsnit i stødår', 
  (p:vHHInd[tot][2085]-@vHHInd[tot][2085])/@nPop[tot][2085] 'Gns. indkomst (vHhInd) - tværsnit i år 60 (2085)'
