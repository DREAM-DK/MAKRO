  a(p:qCR_NR)[2026]-a(@qCR_NR)[2026] 'NR-forbrug fremadskuende husholdning (qCR_NR)'
  file=%shock_dir + 'Forbrug_a.svg';

PLOT <21u 70u ytitle='Marginal forbrugstilbøjelighed' n>
  (a(p:vCHtM_NR)[2026]-a(@vCHtM_NR)[2026]) / (a(p:vHHIndHtM)[2026]-a(@vHHIndHtM)[2026]) 'MPC for HtM-husholdning', 
  (a(p:vCR_NR)[2026]-a(@vCR_NR)[2026]) / (a(p:vHHIndR)[2026]-a(@vHHIndR)[2026]) 'MPC for fremadskuende husholdning', 
