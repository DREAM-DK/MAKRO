   (vHh[RealKred,tot]-@vHh[RealKred,tot]) 'Realkreditgæld (modsat fortegn, da gæld)'
   file=%shock_dir + 'Formue_MAKRO.svg';

PLOT <2035 2090 n ytitle='Andel af vækst i pensionsformue der fortrænges'> 
  (1-(vHhFormue[30]-base:vHhFormue[30])/((vHhPens[30]-base:vHhPens[30]))) '30-årig i fuld model',
  (1-(vHhFormue[40]-base:vHhFormue[40])/((vHhPens[40]-base:vHhPens[40]))) '40-årig i fuld model',
  (1-(vHhFormue[50]-base:vHhFormue[50])/((vHhPens[50]-base:vHhPens[50]))) '50-årig i fuld model',
  (1-(vHhFormue[60]-base:vHhFormue[60])/((vHhPens[60]-base:vHhPens[60]))) '60-årig i fuld model',
  (1-(vHhFormue[70]-base:vHhFormue[70])/((vHhPens[70]-base:vHhPens[70]))) '70-årig i fuld model',
  (1-(vHhFormue[tot]-base:vHhFormue[tot])/((vHhPens[tot]-base:vHhPens[tot]))) 'Aggregeret i fuld model'
 file=%shock_dir + 'Fortrængning_alder_MAKRO.svg';

