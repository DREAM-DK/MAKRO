// =====================================================================================================================
// Gekko-analyse af permanente stød til MAKRO
// =====================================================================================================================

// run Analysis\Shocks_MPC\Grafer_Pension;

// SETTINGS
RESTART;
option freq = a; time 2025 2099;

// Grafer for stød til fuld model
READ <gdx> Model\gdx\stiliseret_grundforloeb.gdx;
%shock_gdx = 'Pension_pshock';
// Indlæsning af relevante banker
READ <gdx first> 'Analysis\Shocks_MPC\gdx\' + %shock_gdx;

%shock_name = 'Pensionsindbetalinger';
%shock_dir = 'Analysis\Shocks_MPC\Shocks\' + %shock_name + '\Grafik\';
%EndYear = 2090;
run Analysis\gcm\Grafer_pshocks;

// Grafer for stød til partiel model (Partiel og fuld model bør have ens løsninger)
READ <gdx ref> Analysis\Shocks_MPC\gdx\partiel.gdx;

READ <gdx> Analysis\Shocks_MPC\gdx\rPensIndb.gdx to p;
// READ <gdx> Analysis\gdx\rPensIndb_f.gdx to f;
// READ <gdx> Analysis\gdx\rPensIndb_b.gdx to b;
// READ <gdx> Analysis\gdx\rPensIndb_lille.gdx to lille;
// READ <gdx> Analysis\gdx\rPensIndb_stor.gdx to stor;
READ <gdx> Analysis\Shocks_MPC\gdx\mpc.gdx to i;
READ <gdx> Analysis\Shocks_MPC\gdx\mpc_permanent.gdx to pi;
READ <gdx> Analysis\Shocks_MPC\gdx\rPensIndb_endo_realkred.gdx to pr;
READ <gdx> Model\gdx\stiliseret_grundforloeb.gdx to base;

// Hjælpevariable så man kan lave årgangs-plot
  // FOR string %i = p, f, b, ref;
FOR string %i = p, ref, pr;
  // Aldersfordelte kohorte-variable
  FOR string %j = vHHInd, vHhIndR, vHHIndHtM, vHhFormueR, vHhFormueHtM, vHhxR, vHhPensR, vHhPensHtM, vFrivaerdiR, 
                  vFrivaerdiHtM, qCR, qCHtM, qCR_NR, qCHtM_NR, qBoligR, qBoligHtM, qFormueBase, dFormue, dArv,
                  vHhNetFinHtM, vHhNetFinR, vCR_NR, vCHtM_NR, vHhFormue, vHhPens;
    CREATE {%i}_{%j}_06;
    CREATE {%i}_{%j}_26;
    CREATE {%i}_{%j}_46;
    CREATE {%i}_{%j}_66;
    FOR val %v = 0 to 73; 
      {%i}_{%j}_06[2026+%v] = {%i}:{%j}[06+%v][2026+%v];
      {%i}_{%j}_26[2026+%v] = {%i}:{%j}[26+%v][2026+%v];
      {%i}_{%j}_46[2026+%v] = {%i}:{%j}[46+%v][2026+%v];
      {%i}_{%j}_66[2026+%v] = {%i}:{%j}[66+%v][2026+%v];
    END; 
  END;
  // Ikke-aldersfordelte kohorte-variable
  // FOR string %j = NetFin, Pens;   
  //   CREATE {%i}_vHH{%j}_06;
  //   CREATE {%i}_vHH{%j}_26;
  //   CREATE {%i}_vHH{%j}_46;
  //   CREATE {%i}_vHH{%j}_66;
  //   FOR val %v = 0 to 73; 
  //     {%i}_vHh{%j}_06[2026+%v] = {%i}:vHh[{%j},06+%v][2026+%v];
  //     {%i}_vHh{%j}_26[2026+%v] = {%i}:vHh[{%j},26+%v][2026+%v];
  //     {%i}_vHh{%j}_46[2026+%v] = {%i}:vHh[{%j},46+%v][2026+%v];
  //     {%i}_vHh{%j}_66[2026+%v] = {%i}:vHh[{%j},66+%v][2026+%v];
  //   END;
  // END; 
  // Fler-dimensionelle variable skåret ned for at kunne alders-plottes
  {%i}_vHHRealKred = series(1);
  {%i}_vHhRealKred[#a] = {%i}:vHh[RealKred,#a];
  {%i}_vHHBankGaeld = series(1);
  {%i}_vHhBankGaeld[#a] = {%i}:vHh[BankGaeld,#a];
  {%i}_vHHNetFin = series(1);
  {%i}_vHhNetFin[#a] = {%i}:vHh[NetFin,#a];
  CREATE {%i}_vHhNetFin_06;
  CREATE {%i}_vHhNetFin_26;
  CREATE {%i}_vHhNetFin_46;
  CREATE {%i}_vHhNetFin_66;
  FOR val %v = 0 to 73; 
    {%i}_vHhNetFin_06[2026+%v] = {%i}_vHhNetFin[06+%v][2026+%v];
    {%i}_vHhNetFin_26[2026+%v] = {%i}_vHhNetFin[26+%v][2026+%v];
    {%i}_vHhNetFin_46[2026+%v] = {%i}_vHhNetFin[46+%v][2026+%v];
    {%i}_vHhNetFin_66[2026+%v] = {%i}_vHhNetFin[66+%v][2026+%v];
  END; 
END;

// Fler-dimensionelle variable skåret ned for at kunne alders-plottes
vHHFormuex = series(1);
vHhFormuex[#a] = vHh[NetFin,#a] - tPensKor * (vHh[pensx,#a] + vHh[kap,#a]);
@vHHFormuex = series(1);
@vHhFormuex[#a] = @vHh[NetFin,#a] - @tPensKor * (@vHh[pensx,#a] + @vHh[kap,#a]);
vHHRealKred = series(1);
vHhRealKred[#a] = vHh[RealKred,#a];
@vHHRealKred = series(1);
@vHhRealKred[#a] = @vHh[RealKred,#a];
vHhBankGaeld = series(1);
vHhBankGaeld[#a] = vHh[BankGaeld,#a];
@vHhBankGaeld = series(1);
@vHhBankGaeld[#a] = @vHh[BankGaeld,#a];
rPensUdb_ = series(1);
rPensUdb_[#a] = rPensUdb[pensx,#a];

// Aldersfordeling fra Arnberg og Barslund (2012)
Arnberg = series(1);
Arnberg[#a] = 0;
FOR val %v = 30 to 34; Arnberg[%v] = 0.01; END; 
FOR val %v = 35 to 39; Arnberg[%v] = 0.09; END; 
FOR val %v = 40 to 44; Arnberg[%v] = 0.13; END; 
FOR val %v = 45 to 49; Arnberg[%v] = 0.29; END; 
FOR val %v = 50 to 54; Arnberg[%v] = 0.26; END; 
Arnberg_lo = series(1);
Arnberg_lo[#a] = 0;
FOR val %v = 30 to 34; Arnberg_lo[%v] = -0.05; END; 
FOR val %v = 35 to 39; Arnberg_lo[%v] = 0.00; END; 
FOR val %v = 40 to 44; Arnberg_lo[%v] = 0.05; END; 
FOR val %v = 45 to 49; Arnberg_lo[%v] = 0.18; END; 
FOR val %v = 50 to 55; Arnberg_lo[%v] = 0.15; END; 
Arnberg_up = series(1);
Arnberg_up[#a] = 0;
FOR val %v = 30 to 34; Arnberg_up[%v] = 0.17; END; 
FOR val %v = 35 to 39; Arnberg_up[%v] = 0.24; END; 
FOR val %v = 40 to 44; Arnberg_up[%v] = 0.22; END; 
FOR val %v = 45 to 49; Arnberg_up[%v] = 0.39; END; 
FOR val %v = 50 to 55; Arnberg_up[%v] = 0.37; END; 

figur_var3 <2032 2090> = (1-(vHhFormue[60]-base:vHhFormue[60])/((vHhPens[60]-base:vHhPens[60])));
figur_var4 <2031 2090> = (1-(vHhFormue[tot]-base:vHhFormue[tot])/((vHhPens[tot]-base:vHhPens[tot])));
PLOT <2026 2090 n ytitle='Andel af vækst i pensionsformue der fortrænges'> 
  (1-(p:vHhFormue[40]-@vHhFormue[40])/((p:vHhPens[40]-@vHhPens[40]))) '40-årig i partiel model',
  (1-(p:vHhFormue[60]-@vHhFormue[60])/((p:vHhPens[60]-@vHhPens[60]))) '60-årig i partiel model',
  figur_var3 '60-årig i fuld model',
  figur_var4 'Aggregeret i fuld model'
  file=%shock_dir + 'Fortrængning_intro.svg';

PLOT <2020 2090 n ytitle='Andel af vækst i pensionsformue der fortrænges'> 
  (1-(p_vHhFormueHtM_46[-20]-ref_vHhFormueHtM_46[-20])/((p_vHhPensHtM_46[-20]-ref_vHhPensHtM_46[-20]))) 'HtM årgang 1980 (20 år før)',
  (1-(p_vHhFormueR_46[-20]-ref_vHhFormueR_46[-20])/((p_vHhPensR_46[-20]-ref_vHhPensR_46[-20]))) 'Fremadskuende årgang 1980 (20 år før)',
  (1-(p_vHhFormueHtM_26-ref_vHhFormueHtM_26)/((p_vHhPensHtM_26-ref_vHhPensHtM_26))) 'HtM årgang 2000',
  (1-(p_vHhFormueR_26-ref_vHhFormueR_26)/((p_vHhPensR_26-ref_vHhPensR_26))) 'Fremadskuende årgang 2000',
  (1-(p_vHhFormueHtM_06[+20]-ref_vHhFormueHtM_06[+20])/((p_vHhPensHtM_06[+20]-ref_vHhPensHtM_06[+20]))) 'HtM årgang 2020 (20 år efter)',
  (1-(p_vHhFormueR_06[+20]-ref_vHhFormueR_06[+20])/((p_vHhPensR_06[+20]-ref_vHhPensR_06[+20]))) 'Fremadskuende årgang 2020 (20 år efter)'
  file=%shock_dir + 'Fortrængning_årgange_HtM&R.svg';

PLOT <2026 2090 n ytitle='Andel af vækst i pensionsformue der fortrænges'> 
  (1-(p:vHhFormue[30]-@vHhFormue[30])/((p:vHhPens[30]-@vHhPens[30]))) '30-årig i partiel model',
  (1-(p:vHhFormue[40]-@vHhFormue[40])/((p:vHhPens[40]-@vHhPens[40]))) '40-årig i partiel model',
  (1-(p:vHhFormue[50]-@vHhFormue[50])/((p:vHhPens[50]-@vHhPens[50]))) '50-årig i partiel model',
  (1-(p:vHhFormue[60]-@vHhFormue[60])/((p:vHhPens[60]-@vHhPens[60]))) '60-årig i partiel model',
  (1-(p:vHhFormue[70]-@vHhFormue[70])/((p:vHhPens[70]-@vHhPens[70]))) '70-årig i partiel model'
  file=%shock_dir + 'Fortrængning_alder.svg';

PLOT <30u 55u ytitle='Andel af vækst i pensionsformue der fortrænges af formue ekskl. friværdi' n>
  0.18 'Fortrængning år 1 - Chetty et al (2014)',
  a(Arnberg_up)[2026] 'Fortrængning gns. år 1-10 - Arnberg og Barslund (2012) - øvre grænse',
  a(Arnberg_lo)[2026] 'Fortrængning gns. år 1-10 - Arnberg og Barslund (2012) - nedre grænse',
  (0-(a(p:vHhx)[2026]-a(@vHhx)[2026])/(a(p:vHhPens)[2026]-a(ref:vHhPens)[2026])) 'Fortrængning år 1  - Partiel MAKRO', 
  (0-(a(p:vHhx)[2035]-a(@vHhx)[2035])/(a(p:vHhPens)[2035]-a(ref:vHhPens)[2035])) 'Fortrængning år 10 - Partiel MAKRO',
  (0-(a(p:vHhx)[2085]-a(@vHhx)[2085])/(a(p:vHhPens)[2085]-a(ref:vHhPens)[2085])) 'Strukturel fortrængning - Partiel MAKRO'
  file=%shock_dir + 'Fortrængning_&vHhx_a.svg';

PLOT <30u 55u ytitle='Andel af vækst i pensionsformue der fortrænges af formue ekskl. friværdi' n>
  0.18 'Fortrængning år 1 - Chetty et al (2014)',
  a(Arnberg_up)[2026] 'Fortrængning gns. år 1-10 - Arnberg og Barslund (2012) - øvre grænse',
  a(Arnberg_lo)[2026] 'Fortrængning gns. år 1-10 - Arnberg og Barslund (2012) - nedre grænse',
  (0-(a(pr:vHhx)[2026]-a(@vHhx)[2026])/(a(pr:vHhPens)[2026]-a(ref:vHhPens)[2026])) 'Fortrængning år 1  - Partiel MAKRO', 
  (0-(a(pr:vHhx)[2035]-a(@vHhx)[2035])/(a(pr:vHhPens)[2035]-a(ref:vHhPens)[2035])) 'Fortrængning år 10 - Partiel MAKRO',
  (0-(a(pr:vHhx)[2085]-a(@vHhx)[2085])/(a(pr:vHhPens)[2085]-a(ref:vHhPens)[2085])) 'Strukturel fortrængning - Partiel MAKRO'
  file=%shock_dir + 'Fortrængning_&vHhx_a_pr.svg';

PLOT <21u 80u ytitle='Effekt pr. 1 kr. ekstra pensionsformue' n>
  (a(p:vHhFormue)[2045]-a(@vHhFormue)[2045])/(a(p:vHhPens)[2045]-a(ref:vHhPens)[2045]) 'Samlet formue - år 20', 
  (a(p_vHhRealKred)[2045]-a(@vHhRealKred)[2045])/(a(p:vHhPens)[2045]-a(ref:vHhPens)[2045]) 'Realkreditgæld - år 20',
  (a(p_vHhBankGaeld)[2045]-a(@vHhBankGaeld)[2045])/(a(p:vHhPens)[2045]-a(ref:vHhPens)[2045]) 'Øvrig gæld - år 20',
  (a(p:vBolig)[2045]-a(@vBolig)[2045])/(a(p:vHhPens)[2045]-a(ref:vHhPens)[2045]) 'Boligformue - år 20', 
  ((a(p:vHhx)[2045]-a(@vHhx)[2045])-(a(p_vHhBankGaeld)[2045]-a(@vHhBankGaeld)[2045]))/(a(p:vHhPens)[2045]-a(ref:vHhPens)[2045]) 'Øvrige aktiver - år 20'
  file=%shock_dir + 'Formueeffekter_a.svg';

PLOT <21u 65u ytitle='MPC og fortrængning' n>
  (1-(a(p:vHhFormue)[2026]-a(@vHhFormue)[2026])/(a(p:vHhPens)[2026]-a(ref:vHhPens)[2026])) 'Gns. fortrængning i år 1 - pensionsstød', 
  1-(a(p:vC_NR)[2026]-a(@vC_NR)[2026]) / (a(p:vHHInd)[2026]-a(@vHHInd)[2026]) '1-MPC - pensionsstød', 
  1-(a(i:vC_NR)[2026]-a(@vC_NR)[2026]) / (a(i:vHHInd)[2026]-a(@vHHInd)[2026]) '1-MPC - midlertidig indkomststød', 
  1-(a(pi:vC_NR)[2026]-a(@vC_NR)[2026]) / (a(pi:vHHInd)[2026]-a(@vHHInd)[2026]) '1-MPC - permanent indkomststød' 
  file=%shock_dir + 'MPC&fortrængning_a.svg';

PLOT <2026 2090 n ytitle='Vækst i formuekomponenter i mio. kr. vækstkorrigeret for 70-årige i fuld model'> 
   (vHhFormue[70]-base:vHhFormue[70]) 'Formue', 
   (vHhPens[70]-base:vHhPens[70]) 'Pensionsformue', 
